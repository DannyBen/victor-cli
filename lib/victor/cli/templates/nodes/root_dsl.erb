<%= "# :root" if ENV['DEBUG'] -%>
# Render this template by running 'ruby FILE' or 'victor render FILE'

require 'victor/script'

<%- if attributes.any? -%>
setup <%= attributes.render %>
<%- end -%>

build do
<%- children.each do |child| -%>
<%= child.render %>
<%- end -%>
end

puts render
save 'output'